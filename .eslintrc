{
  "root": true,
  "env": {
    "node": true
  },
  "plugins": ["@typescript-eslint", "prettier"],
  "extends": ["plugin:vue/vue3-essential", "eslint:recommended", "@vue/typescript/recommended", "@vue/prettier"],
  "parserOptions": {
    "ecmaVersion": 2020,
    "sourceType": "module"
  },
  "rules": {
    "prettier/prettier": 1 // Means warning
  }
}